%!PS-Adobe-2.0 EPSF-2.0
%%BoundingBox: 0 0 130 40
% define the variables used
/height 10 def
/width 10 def
/origin_x 15 def
/origin_y 15 def
/text_y 2 def

% set up the font
/Times-Roman findfont
10 scalefont
setfont

/fill_in_single
{
	2 2 rmoveto
	6 6 rlineto
	-2 0 rmoveto
	-4 -4 rlineto
	0 2 rmoveto
	2 2 rlineto
	0 -6 rmoveto
	4 4 rlineto
	0 -2 rmoveto
	-2 -2 rlineto
	-6 -2 rmoveto

}def
/fill_in_pair
{
	2 2 rmoveto
	16 6 rlineto
	-6 0 rmoveto
	-10 -4 rlineto
	0 2 rmoveto
	4 2 rlineto
	0 -6 rmoveto
	12 4 rlineto
	0 -2 rmoveto
	-6 -2 rlineto
	-2 width sub -2 rmoveto
}def

/make_cache_line
{
	12 width mul 0 rlineto % bottom 
	0 height rlineto % right
	-12 width mul 0 rlineto % top 
	0 -1 height mul rlineto % right

	% draw the first boundary
	10 -1 0
	{
		1 width mul 0 rmoveto
		0 height rlineto
		0 -1 height mul rmoveto
	} for

	% draw the edge of the cache box
	-3 width mul 0 rmoveto
	0 -0.6 height mul rlineto
	0 2.2 height mul rlineto
	-4 width mul 0 rmoveto
	0 -2.2 height mul rlineto
	0 0.6 height mul rmoveto
	-4 width mul 0 rmoveto
	0 -0.6 height mul rlineto
	0 2.2 height mul rlineto
	0 -0.6 height mul rmoveto
	0 -1 height mul rmoveto
}def

	origin_x origin_y moveto % move to start
	make_cache_line
	3 width mul 0 rmoveto
	fill_in_single
	width 0 rmoveto
	3 -1 0 { fill_in_pair width 2 mul 0 rmoveto } for % draw 5 siblings
	origin_x origin_y moveto
	0 height rmoveto % move for the text
	2 5 rmoveto
	1 width mul 0 rmoveto
	(line) show
	2.5 width mul 0 rmoveto
	(line) show
	2.5 width mul 0 rmoveto
	(line) show


stroke
