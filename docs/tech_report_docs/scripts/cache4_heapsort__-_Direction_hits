set key top left Left reverse
set xrange [4096:4194304]
set yrange [0:]
set xtics (8192,16384,32768,65536,131072,262144,524288,1048576,2097152)
set format x "%8.0f"
set logscale x
set data style linespoints
set term postscript eps
set output '../plots/cache4_heapsort__-_Direction_hits.eps'
set xlabel 'set size in keys'
set ylabel 'Direction hits per key'
plot \
