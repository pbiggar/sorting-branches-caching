set key top left Left reverse
set xrange [4096:4194304]
set yrange [0:]
set xtics (8192,16384,32768,65536,131072,262144,524288,1048576,2097152)
set format x "%8.0f"
set logscale x
set data style linespoints
set term postscript eps
set output '../data/plots/demo9-2.eps'
set xlabel 'set size in keys'
set ylabel 'Branches per key'
plot \
'../data/converted_simple_logs/base_heapsort/base_heapsort_sim-bpred_-bpred_bimod_-bpred:bimod_512' using 1:6 title 'base heapsort (2-heap)',\
'../data/converted_simple_logs/memory_tuned4_heapsort/memory_tuned4_heapsort_sim-bpred_-bpred_bimod_-bpred:bimod_512' using 1:6 title 'memory-tuned heapsort (4-heap) (4-heap)',\
'../data/converted_simple_logs/memory_tuned_heapsort/memory_tuned_heapsort_sim-bpred_-bpred_bimod_-bpred:bimod_512' using 1:6 title 'memory-tuned heapsort (8-heap) (8-heap)'
