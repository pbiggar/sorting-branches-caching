set key top left Left reverse
set xrange [4096:4194304]
set yrange [0:]
set xtics (8192,16384,32768,65536,131072,262144,524288,1048576,2097152)
set format x "%8.0f"
set logscale x
set data style linespoints
set term postscript eps
set output '../data/plots/demo9-1.eps'
set xlabel 'set size in keys'
set ylabel 'Branch misses per key'
plot \
'../data/converted_simple_logs/base_heapsort__/base_heapsort__sim-bpred_-bpred_bimod_-bpred:bimod_512' using 1:12 title 'base heapsort (2-heap)',\
'../data/converted_simple_logs/cache4_heapsort__/cache4_heapsort__sim-bpred_-bpred_bimod_-bpred:bimod_512' using 1:12 title 'cache heapsort (4-heap)',\
'../data/converted_simple_logs/cache_heapsort__/cache_heapsort__sim-bpred_-bpred_bimod_-bpred:bimod_512' using 1:12 title 'cache heapsort (8-heap)'
