'../data/converted_simple_logs/improved_shakersort/improved_shakersort_sim-bpred_-bpred_bimod_-bpred:bimod_512' using 1:12 title 'shakersort - bimodal',\
'../data/converted_simple_logs/improved_shakersort/improved_shakersort_sim-bpred_-bpred_2lev_-bpred:2lev_1_512_10_1' using 1:12 title 'shakersort - two-level adaptive'
