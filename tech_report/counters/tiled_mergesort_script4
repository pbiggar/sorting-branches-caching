set term postscript eps
set boxwidth 0.25 absolute
set style fill pattern 0 border -1
set format y "%8.0f"
set yrange [0:156000000]
set ticscale 0 0
set offsets .1, .1, 0, 0
set bmargin 3
set title "tiled_mergesort branch predictor results 4"
set output 'tiled_mergesort-4.eps'
set xtics ("forwards middle" 0, "\nforwards next" 1, "forwards end" 2, "\nforwards setup" 3, "forwards equal" 4, "" 5, "" 6)
plot \
'tiled_mergesort_data4' using ($0-.15):($2+$3) title 'incorrect' with boxes lt -1,\
'tiled_mergesort_data4' using ($0-.15):2 title 'correct' with boxes lt -1,\
'tiled_mergesort_data4' using ($0+.15):($4+$5) title 'not taken' with boxes lt -1,\
'tiled_mergesort_data4' using ($0+.15):4 title 'taken' with boxes lt -1